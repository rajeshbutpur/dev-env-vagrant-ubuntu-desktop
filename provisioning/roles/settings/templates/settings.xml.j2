<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
      <id>evise-maven-releases</id>
      <username>[USERNAME]</username>
      <password>[ENCRYPTED-PASSWORD]</password>
    </server>
    <server>
      <id>evise-maven-dependencies</id>
      <username>[USERNAME]</username>
      <password>[ENCRYPTED-PASSWORD]</password>
    </server>
    <server>
      <id>evise-maven-snapshots</id>
      <username>[USERNAME]</username>
      <password>[ENCRYPTED-PASSWORD]</password>
    </server>
  </servers>
  <profiles>
    <profile>
      <id>evise-repositories</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <repositories>
        <repository>
          <id>evise-maven-dependencies</id>
          <url>https://ci-artifactory.evise-nonprod.com/artifactory/evise-maven-dependencies</url>
          <releases>
            <enabled>true</enabled>
          </releases>
        </repository>
        <repository>
          <id>evise-maven-releases</id>
          <url>https://ci-artifactory.evise-nonprod.com/artifactory/evise-maven-release</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>evise-maven-snapshots</id>
          <url>https://ci-artifactory.evise-nonprod.com/artifactory/evise-maven-snapshots</url>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
          <releases>
            <enabled>false</enabled>
          </releases>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>evise-plugin-repository</id>
          <url>https://ci-artifactory.evise-nonprod.com/artifactory/evise-maven-dependencies/</url>
          <releases>
            <enabled>true</enabled>
          </releases>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
</settings>
